{
  "name": "Kibana Clicker",
  "version": "0.0.0",
  "description": "Automation tool for Kibana",
  "homepage_url": "https://github.com/hyzyla/kibana-clicker",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "applications": {
    "gecko": {
      "id": "kibana-clicker@hyzyla.dev",
      "strict_min_version": "67.0"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "kibana-clicker@hyzyla.dev",
      "strict_min_version": "67.0"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.ts"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
